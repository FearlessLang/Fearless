package base

Str:Sealed{
  .len: UInt,
  // Str is not Stringable due to limitations of the Java codegen target
  .str: Str,
  +(other: Str): Str,
  ==(other: read Str): Bool,
  }
Stringable:{
  .str: Str,
  }
_StrInstance:Str{
  .len -> this.len,
  .str -> this.str,
  +(other) -> this + other,
  ==(other) -> this == other,
  }
