package base.caps

/// Capability for reading and writing to files and devices, including stdout and stderr.
IO:{
  lent .print(msg: Str): Void,
  lent .println(msg: Str): Void,
  lent .printErr(msg: Str): Void,
  lent .printlnErr(msg: Str): Void,
  }
FIO:FCap[RootCap, IO]{ auth -> _IO }
FIO':FCap[IO, IO]{ auth -> auth }
_IO:Sealed,IO{
  .print(msg) -> Abort!,
  .println(msg) -> Abort!,
  .printErr(msg) -> Abort!,
  .printlnErr(msg) -> Abort!,
  }