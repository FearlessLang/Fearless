package base
Str:Sealed{
  // Str is not Stringable due to limitations of the Java codegen target
  .str: Str,
  .len: UInt,
  .isEmpty: Bool,
  +(other: Str): Str,
  ==(other: Str): Bool,
  readOnly .toImm: Str,
  }
Stringable:{
  read .str: Str,
  }
_StrInstance:Str{
  .len -> Magic!,
  .isEmpty -> Magic!,
  .str -> Magic!,
  +(other) -> Magic!,
  ==(other) -> Magic!,
  .toImm -> Magic!,
  }

StrMap[V]:LensMap[Str, mdf V]{ k1, k2 -> k1 == k2 }
