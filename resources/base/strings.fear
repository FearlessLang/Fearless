package base
Str:Sealed{
  .len: UInt,
  // Str is not Stringable due to limitations of the Java codegen target
  .str: Str,
  +(other: Str): Str,
  ==(other: Str): Bool,
  }
Stringable:{
  read .str: Str,
  }
_StrInstance:Str{
  .len -> Magic!,
  .str -> Magic!,
  +(other) -> Magic!,
  ==(other) -> Magic!,
  }