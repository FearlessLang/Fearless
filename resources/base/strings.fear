package base

Str:Sealed{
  read .len: UInt,
  // Str is not Stringable due to limitations of the Java codegen target
  read .str: Str,
  read +(other: read Str): Str,
  read ==(other: read Str): Bool,
  }
Stringable:{
  read .str: Str,
  }
_StrInstance:Str{
  .len -> this.len,
  .str -> this.str,
  +(other) -> this + other,
  ==(other) -> this == other,
  }
